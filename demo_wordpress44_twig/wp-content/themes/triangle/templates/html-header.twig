<!doctype html>
        <!--[if lt IE 7]><html class="no-js ie ie6 lt-ie9 lt-ie8 lt-ie7" {{site.language_attributes}}> <![endif]-->
        <!--[if IE 7]><html class="no-js ie ie7 lt-ie9 lt-ie8" {{site.language_attributes}}> <![endif]-->
        <!--[if IE 8]><html class="no-js ie ie8 lt-ie9" {{site.language_attributes}}> <![endif]-->
<!--[if gt IE 8]><!--><html class="no-js" {{site.language_attributes}}> <!--<![endif]-->
    <head>
        <meta charset="{{site.charset}}" />
        <title>   
            {% if wp_title %}
                {{ wp_title }} - {{ site.name }}
            {% else %}
                {{ site.name }}
            {% endif %}
        </title>
        <link href="{{site.theme.uri}}/css/bootstrap.min.css" rel="stylesheet">
        <link href="{{site.theme.uri}}/css/font-awesome.min.css" rel="stylesheet">
        <link href="{{site.theme.uri}}/css/animate.min.css" rel="stylesheet"> 
        <link href="{{site.theme.uri}}/css/lightbox.css" rel="stylesheet"> 
        <link href="{{site.theme.uri}}/css/main.css" rel="stylesheet">
        <link href="{{site.theme.uri}}/css/responsive.css" rel="stylesheet">
        <link href="{{site.theme.uri}}/css/custom.css" rel="stylesheet">
        <!--[if lt IE 9]>
                <script src="{{site.theme.uri}}/js/html5shiv.js"></script>
                <script src="{{site.theme.uri}}/js/respond.min.js"></script>
        <![endif]-->       
        <link rel="shortcut icon" href="{{site.theme.uri}}/images/ico/favicon.ico">
        <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{site.theme.uri}}/images/ico/apple-touch-icon-144-precomposed.png">
        <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{site.theme.uri}}/images/ico/apple-touch-icon-114-precomposed.png">
        <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{site.theme.uri}}/images/ico/apple-touch-icon-72-precomposed.png">
        <link rel="apple-touch-icon-precomposed" href="{{site.theme.uri}}/images/ico/apple-touch-icon-57-precomposed.png">
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="pingback" href="{{site.pingback_url}}" />
        {{function('wp_head')}}
    </head><!--/head-->

    <body class="{{body_class}}" data-template="base.twig" >
        <header id="header">      
            <div class="container">
                <div class="row">
                    <div class="col-sm-12 overflow">
                        <div class="social-icons pull-right">
                            <ul class="nav nav-pills">
                                <li><a target="_blank"  href="{{s_facebook}}"><i class="fa fa-facebook"></i></a></li>
                                <li><a target="_blank" href="{{s_twitter}}"><i class="fa fa-twitter"></i></a></li>
                                <li><a target="_blank" href="{{s_linkedin}}"><i class="fa fa-linkedin"></i></a></li>
                            </ul>
                        </div> 
                    </div>
                </div>
            </div>
            <div class="navbar navbar-inverse" role="banner">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="{{site.url}}">
                            <h1>
                                <img src="{{site_logo}}" alt="{{site.name}}" title="{{site.name}}" class="img-responsive" />
                            </h1>
                        </a>
                    </div>
                    <div class="collapse navbar-collapse">
                        <ul class="nav navbar-nav navbar-right">
                            {% for item in menu.get_items %}
                                     {% if item.get_children is empty %}
                                        <li><a href="{{item.get_link}}">{{item.title}}</a></li>
                                    {% elseif item.get_children %}
                                    <li class="dropdown"><a href="{{item.get_link}}">{{item.title}} <i class="fa fa-angle-down"></i></a>
                                        <ul role="menu" class="sub-menu">
                                            {% for child in item.get_children %}
                                                <li><a href="{{child.get_link}}">{{child.title}}</a></li>
                                                {% endfor %}
                                        </ul>
                                    </li>        
                                {% endif %}
                            {% endfor %}
                        </ul>
                    </div>
                        {{function("get_search_form")}}
                </div>
            </div>
        </header>
<!--/#header-->